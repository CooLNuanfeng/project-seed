{
  "name": "{{name}}",
  "version": "{{version}}",
  "author": "{{author}}",
  "description": "{{description}}",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "webpack --config build/webpack.prod.conf.js"
  },
  "dependencies": [
    "vue"{{#if router}},
    "vue-router"{{/if}}{{#if store}},
    "vuex"{{/if}}
  ],
  "devDependencies": [
    "autoprefixer",
    "clean-webpack-plugin",
    "file-loader",
    "postcss-loader",
    "style-loader",
    "url-loader",
    "vue",
    "vue-loader"{{#if router}},
    "vue-router",{{/if}}{{#if store}}
    "vuex",{{/if}}
    "webpack",
    "webpack-merge",
    "babel-core",
    "babel-loader",
    "babel-preset-env",
    "cross-env",
    "css-loader",
    "extract-text-webpack-plugin",
    "html-webpack-plugin"{{#if less}},
    "less",
    "less-loader",{{/if}}
    "vue-template-compiler",
    "webpack-cli",
    "webpack-dev-server"
  ]
}
